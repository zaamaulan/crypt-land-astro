---
import { cn } from "@/lib/utils";

interface Props {
  className?: string;
}

const { className } = Astro.props;
---

<figure class={cn("flex items-center gap-5", className)}>
  <img src="/logo.svg" alt="logo" class="h-[50px] w-[50px]" loading="eager" />
  <figcaption class="text-3xl text-white">Crypt Land</figcaption>
</figure>
